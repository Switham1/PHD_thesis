  author = {Tolhuis, Bas and Palstra, Robert-Jan and Splinter, Erik and Grosveld, Frank and de Laat, Wouter},
  date = {2002-12-01},
  journaltitle = {Molecular Cell},
  shortjournal = {Molecular Cell},
  volume = {10},
