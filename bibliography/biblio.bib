  keywords = {Botany,cloning,Cloning; Molecular,Deoxyribonucleases; Type II Site-Specific,DNA,DNA assembly,Eukaryota,Genetic Engineering,genetic syntax,GoldenGate,Plants,Plants; Genetically Modified,Plasmids,Reference Standards,synthetic biology,Synthetic Biology,Transcription; Genetic,Type IIS restriction endonucleases},
  langid = {english},
