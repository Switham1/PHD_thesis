@article{kulkarniInferencePlantGene2020,
  title = {Inference of Plant Gene Regulatory Networks Using Data-Driven Methods: {{A}} Practical Overview},
  shorttitle = {Inference of Plant Gene Regulatory Networks Using Data-Driven Methods},
  author = {Kulkarni, Shubhada R. and Vandepoele, Klaas},
  date = {2020-06-01},
  journaltitle = {Biochimica et Biophysica Acta (BBA) - Gene Regulatory Mechanisms},
