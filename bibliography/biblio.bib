@article{liuDependencyCellularProtein2016,
  title = {On the {{Dependency}} of {{Cellular Protein Levels}} on {{mRNA Abundance}}},
  author = {Liu, Yansheng and Beyer, Andreas and Aebersold, Ruedi},
