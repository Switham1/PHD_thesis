@article{marnettoEvolutionaryRewiringHuman2018,
  title = {Evolutionary {{Rewiring}} of {{Human Regulatory Networks}} by {{Waves}} of {{Genome Expansion}}},
  author = {Marnetto, Davide and Mantica, Federica and Molineris, Ivan and Grassi, Elena and Pesando, Igor and Provero, Paolo},
  date = {2018-02-01},
  journaltitle = {The American Journal of Human Genetics},
  shortjournal = {The American Journal of Human Genetics},
  volume = {102},
  pages = {207--218},
  issn = {0002-9297},
  doi = {10.1016/j.ajhg.2017.12.014},
  url = {http://www.sciencedirect.com/science/article/pii/S0002929717305037},
  urldate = {2019-04-10},
  abstract = {Genome expansion is believed to be an important driver of the evolution of gene regulation. To investigate the role of a newly arising sequence in rewiring regulatory networks, we estimated the age of each region of the human genome by applying maximum parsimony to genome-wide alignments with 100 vertebrates. We then studied the age distribution of several types of functional regions, with a focus on regulatory elements. The age distribution of regulatory elements reveals the extensive use of newly formed genomic sequence in the evolution of regulatory interactions. Many transcription factors have expanded their repertoire of targets through waves of genomic expansions that can be traced to specific evolutionary times. Repeated elements contributed a major part of such expansion: many classes of such elements are enriched in binding sites of one or a few specific transcription factors, whose binding sites are localized in specific portions of the element and characterized by distinctive motif words. These features suggest that the binding sites were available as soon as the new sequence entered the genome, rather than being created later by accumulation of point mutations. By comparing the age of regulatory regions to the evolutionary shift in expression of nearby genes, we show that rewiring through genome expansion played an important role in shaping human regulatory networks.},
  file = {/run/user/1000/gvfs/google-drive\:host=gmail.com,user=sam.switham/1n1_S0njsC6VYDga_eTedxbmHc4pnYn0o/Marnetto et al_2018_Evolutionary Rewiring of Human Regulatory Networks by Waves of Genome Expansion.pdf;/home/witham/zotero/Zotero/storage/ME42DU2J/S0002929717305037.html},
  keywords = {evolution,repeated genomic elements,transcriptional regulation},
  number = {2}
}

@article{maronEffect5hydroxytryptophanCholecystokinin4induced2004,
