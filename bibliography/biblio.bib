  file = {/run/user/1000/gvfs/google-drive\:host=gmail.com,user=sam.switham/1n1_S0njsC6VYDga_eTedxbmHc4pnYn0o/Pajoro et al_2018_Profiling Nucleosome Occupancy by MNase-seq.pdf},
