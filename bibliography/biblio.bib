  number = {51}
}

@article{ganjiDNABindingProteins2016,
  title = {{{DNA}} Binding Proteins Explore Multiple Local Configurations during Docking via Rapid Rebinding},
  author = {Ganji, Mahipal and Docter, Margreet and Le Grice, Stuart F.J. and Abbondanzieri, Elio A.},
  date = {2016-09-30},
  journaltitle = {Nucleic Acids Research},
  shortjournal = {Nucleic Acids Res},
  volume = {44},
  pages = {8376--8384},
  issn = {0305-1048},
  doi = {10.1093/nar/gkw666},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5041478/},
  urldate = {2019-03-27},
  abstract = {Finding the target site and associating in a specific orientation are essential tasks for DNA-binding proteins. In order to make the target search process as efficient as possible, proteins should not only rapidly diffuse to the target site but also dynamically explore multiple local configurations before diffusing away. Protein flipping is an example of this second process that has been observed previously, but the underlying mechanism of flipping remains unclear. Here, we probed the mechanism of protein flipping at the single molecule level, using HIV-1 reverse transcriptase (RT) as a model system. In order to test the effects of long-range attractive forces on flipping efficiency, we varied the salt concentration and macromolecular crowding conditions. As expected, increased salt concentrations weaken the binding of RT to DNA while increased crowding strengthens the binding. Moreover, when we analyzed the flipping kinetics, i.e. the rate and probability of flipping, at each condition we found that flipping was more efficient when RT bound more strongly. Our data are consistent with a view that DNA bound proteins undergo multiple rapid re-binding events, or short hops, that allow the protein to explore other configurations without completely dissociating from the DNA.},
  eprint = {27471033},
  eprinttype = {pmid},
  file = {/run/user/1000/gvfs/google-drive\:host=gmail.com,user=sam.switham/1n1_S0njsC6VYDga_eTedxbmHc4pnYn0o/Ganji et al_2016_DNA binding proteins explore multiple local configurations during docking via.pdf},
  number = {17},
  pmcid = {PMC5041478}
}

@article{ganMultipleReferenceGenomes2011,
  title = {Multiple Reference Genomes and Transcriptomes for {{{\emph{Arabidopsis}}}}{\emph{ Thaliana}}},
  author = {Gan, Xiangchao and Stegle, Oliver and Behr, Jonas and Steffen, Joshua G. and Drewe, Philipp and Hildebrand, Katie L. and Lyngsoe, Rune and Schultheiss, Sebastian J. and Osborne, Edward J. and Sreedharan, Vipin T. and Kahles, André and Bohnert, Regina and Jean, Géraldine and Derwent, Paul and Kersey, Paul and Belfield, Eric J. and Harberd, Nicholas P. and Kemen, Eric and Toomajian, Christopher and Kover, Paula X. and Clark, Richard M. and Rätsch, Gunnar and Mott, Richard},
