  date = {2012-01-01},
  journaltitle = {Asian Pacific Journal of Tropical Biomedicine},
  shortjournal = {Asian Pacific Journal of Tropical Biomedicine},
  volume = {2},
  pages = {S1945-S1953},
  issn = {2221-1691},
  doi = {10.1016/S2221-1691(12)60524-3},
