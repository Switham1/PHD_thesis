@incollection{zhenMethodsDetectSelection2012,
  title = {Methods to {{Detect Selection}} on {{Noncoding DNA}}},
  booktitle = {Evolutionary {{Genomics}}: {{Statistical}} and {{Computational Methods}}, {{Volume}} 2},
  author = {Zhen, Ying and Andolfatto, Peter},
  editor = {Anisimova, Maria},
  date = {2012},
