@article{barrettRegulationEukaryoticGene2012,
  title = {Regulation of Eukaryotic Gene Expression by the Untranslated Gene Regions and Other Non-Coding Elements},
  author = {Barrett, Lucy W. and Fletcher, Sue and Wilton, Steve D.},
  date = {2012-11-01},
  journaltitle = {Cellular and Molecular Life Sciences},
