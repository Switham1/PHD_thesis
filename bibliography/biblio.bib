@article{dingPlantFactoryMoth2014,
  title = {A Plant Factory for Moth Pheromone Production},
  author = {Ding, Bao-Jian and Hofvander, Per and Wang, Hong-Lei and Durrett, Timothy P. and Stymne, Sten and Löfstedt, Christer},
  date = {2014-02-25},
  journaltitle = {Nature Communications},
  volume = {5},
  pages = {3353},
  doi = {10.1038/ncomms4353},
  url = {https://www.nature.com/articles/ncomms4353},
  urldate = {2019-03-31},
  abstract = {Moths depend on pheromone communication for mate finding and synthetic pheromones are used for monitoring or disruption of pheromone communication in pest insects. Here we produce moth sex pheromone, using Nicotiana benthamiana as a plant factory, by transient expression of up to four genes coding for consecutive biosynthetic steps. We specifically produce multicomponent sex pheromones for two species. The fatty alcohol fractions from the genetically modified plants are acetylated to mimic the respective sex pheromones of the small ermine moths Yponomeuta evonymella and Y. padella. These mixtures are very efficient and specific for trapping of male moths, matching the activity of conventionally produced pheromones. Our long-term vision is to design tailor-made production of any moth pheromone component in genetically modified plants. Such semisynthetic preparation of sex pheromones is a novel and cost-effective way of producing moderate to large quantities of pheromones with high purity and a minimum of hazardous waste.},
  file = {/run/user/1000/gvfs/google-drive\:host=gmail.com,user=sam.switham/1n1_S0njsC6VYDga_eTedxbmHc4pnYn0o/Ding et al_2014_A plant factory for moth pheromone production.pdf;/home/witham/zotero/Zotero/storage/KXH8NZRB/ncomms4353.html},
