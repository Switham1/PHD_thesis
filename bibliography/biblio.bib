@article{nikolovResolvingBackboneBrassicaceae2019,
  title = {Resolving the Backbone of the {{Brassicaceae}} Phylogeny for Investigating Trait Diversity},
  author = {Nikolov, Lachezar A. and Shushkov, Philip and Nevado, Bruno and Gan, Xiangchao and Al‐Shehbaz, Ihsan A. and Filatov, Dmitry and Bailey, C. Donovan and Tsiantis, Miltos},
  date = {2019},
  journaltitle = {New Phytologist},
  volume = {222},
  pages = {1638--1651},
