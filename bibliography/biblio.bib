@article{levineTranscriptionalEnhancersAnimal2010,
  title = {Transcriptional {{Enhancers}} in {{Animal Development}} and {{Evolution}}},
  author = {Levine, Mike},
  date = {2010-09-14},
  journaltitle = {Current Biology},
  shortjournal = {Current Biology},
