  KEYWORDS = {Arabidopsis,Chromosome Mapping,DNA; Plant,Gene Expression Regulation; Plant,Genome; Plant,Oligonucleotide Array Sequence Analysis,Promoter Regions; Genetic,Sequence Analysis; DNA,TATA Box,Transcription Initiation Site},
