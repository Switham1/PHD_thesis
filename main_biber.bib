  FILE = {/home/witham/zotero/Zotero/storage/N8VIDKA8/Chaubet et al. - 1996 - Identification of cis-elements regulating the expr.pdf;/home/witham/zotero/Zotero/storage/H8YIPTW4/j.1365-313X.1996.10030425.html},
  ISSN = {1365-313X},
