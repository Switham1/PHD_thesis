  ISSN = {1553-7358},
  JOURNALTITLE = {PLOS Computational Biology},
  KEYWORDS = {Caenorhabditis elegans,Chromatin,Drosophila melanogaster,Genomic signal processing,Invertebrate genomics,Nucleosome mapping,Nucleosomes,Sequence motif analysis},
  LANGID = {english},
