  ISSN = {0888-7543},
  JOURNALTITLE = {Genomics},
  KEYWORDS = {Conformational diversity,dN/dS ratio,Functional divergence,Housekeeping hubs,Multi-interface hubs,Single-interface hubs,Tissue-specific hubs},
