  ISSN = {1365-2664},
  JOURNALTITLE = {Journal of Applied Ecology},
  KEYWORDS = {agroecosystems,fertilizer,functional traits,nitrogen cycling,nitrogen losses,plant mixtures,plant traits,plant–soil interactions},
