  FILE = {/home/witham/zotero/Zotero/storage/X3JPDNNA/Sedeek et al. - 2019 - Plant Genome Engineering for Targeted Improvement .pdf},
  ISSN = {1664-462X},
  JOURNALTITLE = {Frontiers in Plant Science},
  KEYWORDS = {Climate Change,CRiSPR/Cas,Crop Improvement,Food security,Genome editing,genome engineering,Synthetic Biology},
  LANGID = {english},
  SHORTJOURNAL = {Front. Plant Sci.},
  TITLE = {Plant {{Genome Engineering}} for {{Targeted Improvement}} of {{Crop Traits}}},
  URLDATE = {2021-04-20},
