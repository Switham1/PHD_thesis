  LANGID = {english},
  SHORTJOURNAL = {Front. Plant Sci.},
  TITLE = {Phylogenetic {{Analyses}} and {{GAGA}}-{{Motif Binding Studies}} of {{BBR}}/{{BPC Proteins Lend}} to {{Clues}} in {{GAGA}}-{{Motif Recognition}} and a {{Regulatory Role}} in {{Brassinosteroid Signaling}}},
