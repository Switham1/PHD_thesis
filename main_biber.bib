  ISSN = {1664-462X},
  JOURNALTITLE = {Frontiers in Plant Science},
  KEYWORDS = {Basic Pentacysteine transcription factors,BBR/BPC proteins,brassinosteroid signaling,GAGA-binding domain,GAGA-factors (GAF),Polycomb repressive complexes,PRE},
