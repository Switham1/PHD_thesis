  FILE = {/home/witham/zotero/Zotero/storage/ZDF4NFY7/Castro Marín et al. - 2011 - Nitrate regulates floral induction in Arabidopsis,.pdf},
  ISSN = {1432-2048},
  JOURNALTITLE = {Planta},
  LANGID = {english},
  NUMBER = {3},
  PAGES = {539--552},
  SHORTJOURNAL = {Planta},
  TITLE = {Nitrate Regulates Floral Induction in {{Arabidopsis}}, Acting Independently of Light, Gibberellin and Autonomous Pathways},
